{
  "private": true,
  "engines": {
    "node": ">=20"
  },
  "packageManager": "yarn@3.6.0",
  "scripts": {
    "dev": "concurrently --prefix-colors auto \"yarn:dev:*\"",
    "dev:cli": "yarn workspace @prismicio/cli dev",
    "dev:manager": "yarn workspace @prismicio/manager dev",
    "dev:plugin-kit": "yarn workspace @prismicio/plugin-kit dev",
    "dev:adapter-next": "yarn workspace @prismicio/adapter-next dev",
    "dev:adapter-nuxt": "yarn workspace @prismicio/adapter-nuxt dev",
    "dev:adapter-nuxt2": "yarn workspace @prismicio/adapter-nuxt2 dev",
    "dev:adapter-sveltekit": "yarn workspace @prismicio/adapter-sveltekit dev",
    "build": "yarn workspaces foreach --topological-dev --verbose run build",
    "types": "yarn workspaces foreach --parallel --verbose run types",
    "lint": "yarn workspaces foreach --parallel --verbose run lint",
    "audit": "yarn workspaces foreach --parallel --verbose run audit",
    "depcheck": "yarn workspaces foreach --parallel --verbose run depcheck",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "publish": "tsx scripts/publish.ts",
    "clean": "rimraf --glob '{packages/*/{dist,package.tgz}}'"
  },
  "dependenciesMeta": {
    "msw": {
      "built": true
    }
  },
  "devDependencies": {
    "@types/semver": "7.3.13",
    "chalk": "4.1.2",
    "concurrently": "7.6.0",
    "execa": "7.1.1",
    "mri": "1.2.0",
    "prettier": "3.0.3",
    "prettier-plugin-jsdoc": "1.1.1",
    "rimraf": "5.0.5",
    "semver": "7.3.8",
    "tsx": "4.6.2"
  },
  "workspaces": [
    "playwright",
    "packages/*"
  ]
}
