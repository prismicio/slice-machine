{
	"name": "@prismicio/adapter-sveltekit",
	"version": "0.0.1",
	"description": "Prismic adapter for SvelteKit.",
	"keywords": [
		"typescript",
		"prismic"
	],
	"repository": {
		"type": "git",
		"url": "ssh://git@github.com/prismicio/devtools.git",
		"directory": "packages/adapter-sveltekit"
	},
	"license": "Apache-2.0",
	"author": "Prismic <contact@prismic.io> (https://prismic.io)",
	"type": "module",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"require": "./dist/index.cjs",
			"import": "./dist/index.js"
		},
		"./package.json": "./package.json"
	},
	"main": "dist/index.cjs",
	"module": "dist/index.js",
	"types": "dist/index.d.ts",
	"typesVersions": {
		"*": {
			"*": [
				"dist/index.d.ts"
			]
		}
	},
	"files": [
		"dist",
		"src"
	],
	"scripts": {
		"build": "vite build",
		"dev": "concurrently --prefix-colors='magenta,blue' yarn:dev:vite yarn:dev:svelte",
		"dev:vite": "vite build --watch",
		"format": "prettier --write .",
		"lint": "eslint --max-warnings 0 --ext .js,.ts .",
		"prepack": "$npm_execpath run build",
		"test": "yarn lint && yarn types && yarn build",
		"types": "tsc --noEmit",
		"depcheck": "depcheck --config=.depcheckrc",
		"audit": "yarn npm audit --environment production --severity high"
	},
	"dependencies": {
		"@prismicio/plugin-kit": "workspace:*",
		"@prismicio/types-internal": "3.11.2",
		"common-tags": "^1.8.2",
		"fp-ts": "^2.13.1",
		"io-ts": "^2.2.20",
		"io-ts-types": "^0.5.19",
		"magicast": "0.3.5",
		"monocle-ts": "^2.3.13",
		"newtype-ts": "^0.3.5",
		"pascal-case": "^3.1.2",
		"prettier-plugin-svelte": "3.4.0"
	},
	"devDependencies": {
		"@sveltejs/kit": "2.20.6",
		"@sveltejs/package": "2.3.1",
		"@sveltejs/vite-plugin-svelte": "4.0.4",
		"@types/common-tags": "1.8.1",
		"@typescript-eslint/eslint-plugin": "5.55.0",
		"@typescript-eslint/parser": "5.55.0",
		"concurrently": "8.2.1",
		"depcheck": "1.4.3",
		"eslint": "8.37.0",
		"eslint-config-prettier": "9.0.0",
		"eslint-plugin-prettier": "5.0.1",
		"eslint-plugin-svelte": "2.33.0",
		"eslint-plugin-tsdoc": "0.2.17",
		"prettier": "3.0.3",
		"prettier-plugin-jsdoc": "1.1.1",
		"svelte": "5.33.5",
		"typescript": "4.9.5",
		"vite": "5.4.20",
		"vite-plugin-sdk": "0.1.1"
	},
	"peerDependencies": {
		"@sveltejs/kit": "^1 || ^2",
		"prettier": ">=3",
		"prettier-plugin-svelte": ">=3",
		"svelte": "^3.54.0 || ^4.0.0-next.0 || ^5.0.0-next.1"
	},
	"peerDependenciesMeta": {
		"prettier": {
			"optional": true
		},
		"prettier-plugin-svelte": {
			"optional": true
		}
	},
	"engines": {
		"node": ">=20"
	},
	"publishConfig": {
		"access": "public"
	}
}
