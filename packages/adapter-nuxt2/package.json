{
	"name": "@prismicio/adapter-nuxt2",
	"version": "0.0.1",
	"description": "Prismic adapter for Nuxt 2.",
	"keywords": [
		"typescript",
		"prismic"
	],
	"repository": {
		"type": "git",
		"url": "ssh://git@github.com/prismicio/devtools.git",
		"directory": "packages/adapter-nuxt2"
	},
	"license": "Apache-2.0",
	"author": "Prismic <contact@prismic.io> (https://prismic.io)",
	"type": "module",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"require": "./dist/index.cjs",
			"import": "./dist/index.js"
		},
		"./package.json": "./package.json"
	},
	"main": "dist/index.cjs",
	"module": "dist/index.js",
	"types": "dist/index.d.ts",
	"typesVersions": {
		"*": {
			"*": [
				"dist/index.d.ts"
			]
		}
	},
	"files": [
		"dist",
		"src"
	],
	"engines": {
		"node": ">=20.19.0"
	},
	"publishConfig": {
		"access": "public"
	},
	"scripts": {
		"build": "vite build",
		"dev": "vite build --watch",
		"types": "tsc --noEmit",
		"format": "prettier --write .",
		"lint": "eslint --max-warnings 0 --ext .js,.ts .",
		"prepack": "$npm_execpath run build",
		"depcheck": "depcheck --config=.depcheckrc",
		"audit": "yarn npm audit --environment production --severity high"
	},
	"dependencies": {
		"@prismicio/plugin-kit": "workspace:*",
		"common-tags": "1.8.2",
		"magicast": "0.3.4",
		"pascal-case": "3.1.2"
	},
	"devDependencies": {
		"@typescript-eslint/eslint-plugin": "5.55.0",
		"@typescript-eslint/parser": "5.55.0",
		"depcheck": "1.4.3",
		"eslint": "8.37.0",
		"eslint-config-prettier": "9.0.0",
		"eslint-plugin-prettier": "5.0.1",
		"eslint-plugin-tsdoc": "0.2.17",
		"nuxt": "2.16.3",
		"prettier": "3.7.1",
		"prettier-plugin-jsdoc": "1.7.0",
		"typescript": "4.9.5",
		"vite": "5.4.20",
		"vite-plugin-sdk": "0.1.1"
	},
	"peerDependencies": {
		"nuxt": "^2.0.0"
	}
}
