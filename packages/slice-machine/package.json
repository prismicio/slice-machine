{
  "name": "slice-machine-ui",
  "version": "0.4.2-alpha.8",
  "private": false,
  "license": "MIT",
  "description": "A visual builder for your Slice Models with all the tools you need to generate data models and mock CMS content locally.",
  "_moduleAliases": {
    "@lib": "build/lib",
    "@models": "build/lib/models"
  },
  "engines": {
    "node": ">=12"
  },
  "scripts": {
    "dev": "concurrently \"next dev\" \"npm run dev --prefix ../../e2e-projects/next\"",
    "test": "jest",
    "watch-test": "jest --watch",
    "config": "tsc --showConfig",
    "clean-build": "rimraf ./build/client",
    "postinstall": "node postinstall",
    "prepublishOnly": "npm run bundle",
    "dev-server": "cross-env PORT=9999 CWD=../../e2e-projects/next ENV=SM concurrently \"tsc -w --project server/tsconfig.json\" \"nodemon -e ts,js --watch server --watch lib --ignore build/ build/server/src/index.js\"",
    "prod-server": "cross-env PORT=9999 CWD=../../e2e-projects/next concurrently \"tsc -w --project server/tsconfig.json\" \"nodemon -e ts,js --watch server --watch lib --ignore build/ build/server/src/index.js\"",
    "build": "npm run build-scripts && npm run build-server && npm run build-next-app",
    "build-scripts": "tsc --project scripts/tsconfig.json",
    "build-server": "tsc --project server/tsconfig.json",
    "build-next-app": "cross-env NODE_OPTIONS=--max_old_space_size=8192 next build",
    "export-build": "next export && npm run clean-build",
    "bundle": "npm run build && npm run export-build",
    "lint": "eslint --cache --cache-location .caches/eslint --cache-strategy content --ext .ts,.tsx,.js,.jsx .",
    "lint:precommit": "eslint",
    "audit": "better-npm-audit audit -l high -p",
    "audit-fix": "npm audit fix"
  },
  "bin": {
    "start-slicemachine": "bin/start.js"
  },
