{
	"name": "@slicemachine/init",
	"version": "2.1.1-dev-hotfixes.0",
	"description": "Init Prismic Slice Machine in your project",
	"keywords": [
		"typescript",
		"prismic"
	],
	"repository": {
		"type": "git",
		"url": "ssh://git@github.com/prismicio/slice-machine.git",
		"directory": "packages/init"
	},
	"license": "Apache-2.0",
	"author": "Prismic <contact@prismic.io> (https://prismic.io)",
	"type": "module",
	"exports": {
		".": {
			"require": "./dist/index.cjs",
			"import": "./dist/index.js"
		},
		"./cli": {
			"require": "./dist/cli.cjs",
			"import": "./dist/cli.js"
		},
		"./package.json": "./package.json"
	},
	"main": "dist/index.cjs",
	"module": "dist/index.js",
	"types": "dist/index.d.ts",
	"bin": {
		"slicemachine-init": "./bin/slicemachine-init.js"
	},
	"files": [
		"bin",
		"dist",
		"src"
	],
	"scripts": {
		"build": "vite build",
		"dev": "vite build --watch",
		"format": "prettier --write .",
		"lint": "eslint --ext .js,.ts .",
		"prepublishOnly": "npm run build",
		"size": "size-limit",
		"test": "npm run lint && npm run types && npm run unit && npm run build && npm run size",
		"types": "tsc --noEmit",
		"unit": "vitest run --coverage",
		"unit:watch": "vitest watch",
		"depcheck": "depcheck --config=.depcheckrc",
		"audit": "better-npm-audit audit -l high -p"
	},
	"dependencies": {
		"@antfu/ni": "^0.20.0",
		"@lihbr/listr-update-renderer": "^0.5.3",
		"@slicemachine/manager": "0.3.1-dev-hotfixes.0",
		"chalk": "^4.1.2",
		"globby": "^13.1.3",
		"listr": "^0.14.3",
		"log-symbols": "^4.1.0",
		"meow": "^11.0.0",
		"open": "^8.4.2",
		"prompts": "^2.4.2",
		"semver": "^7.3.8"
	},
	"devDependencies": {
		"@size-limit/preset-small-lib": "^8.2.4",
		"@slicemachine/plugin-kit": "0.3.1-dev-hotfixes.0",
		"@types/listr": "^0.14.4",
		"@types/prompts": "^2.4.3",
		"@types/semver": "^7.3.13",
		"@typescript-eslint/eslint-plugin": "^5.55.0",
		"@typescript-eslint/parser": "^5.55.0",
		"@vitest/coverage-c8": "^0.27.3",
		"better-npm-audit": "^3.7.3",
		"depcheck": "^1.4.3",
		"eslint": "^8.37.0",
		"eslint-config-prettier": "^8.7.0",
		"eslint-plugin-prettier": "^4.2.1",
		"eslint-plugin-tsdoc": "^0.2.17",
		"execa": "^7.1.1",
		"hook-std": "^3.0.0",
		"memfs": "^3.4.13",
		"mock-stdin": "^1.0.0",
		"msw": "^1.1.0",
		"parse-multipart-data": "^1.5.0",
		"prettier-plugin-jsdoc": "^0.4.2",
		"size-limit": "^8.2.4",
		"typescript": "^4.9.5",
		"vite": "^4.1.4",
		"vite-plugin-sdk": "^0.1.1",
		"vitest": "^0.27.3"
	}
}
