{
	"name": "start-slicemachine",
	"version": "0.1.0",
	"private": true,
	"description": "Start Slice Machine from within a project.",
	"repository": {
		"type": "git",
		"url": "ssh://git@github.com/prismicio/slice-machine.git"
	},
	"license": "Apache-2.0",
	"author": "Prismic <contact@prismic.io> (https://prismic.io)",
	"sideEffects": false,
	"type": "module",
	"exports": {
		".": {
			"require": "./dist/index.cjs",
			"import": "./dist/index.js"
		},
		"./package.json": "./package.json"
	},
	"main": "dist/index.cjs",
	"module": "dist/index.js",
	"types": "dist/index.d.ts",
	"files": [
		"dist",
		"src"
	],
	"scripts": {
		"build": "vite build",
		"dev": "vite build --watch",
		"format": "prettier --write .",
		"lint": "eslint --ext .js,.ts .",
		"prepare": "npm run build",
		"size": "size-limit",
		"test": "npm run lint && npm run unit && npm run build && npm run size",
		"unit": "vitest run --coverage",
		"unit:watch": "vitest watch",
		"depcheck": "depcheck --config=.depcheckrc"
	},
	"bin": {
		"start-slicemachine": "./bin/start-slicemachine.js"
	},
	"dependencies": {
		"@slicemachine/manager": "0.1.0",
		"chalk": "^4.1.2",
		"cors": "^2.8.5",
		"h3": "^1.0.2",
		"mri": "^1.2.0",
		"node-fetch": "^3.3.0",
		"open": "^8.4.0",
		"serve-static": "^1.15.0"
	},
	"devDependencies": {
		"@size-limit/preset-small-lib": "^8.1.0",
		"@types/cors": "^2.8.13",
		"@types/serve-static": "^1.15.0",
		"@typescript-eslint/eslint-plugin": "~5.48.0",
		"@typescript-eslint/parser": "^5.48.0",
		"@vitest/coverage-c8": "^0.26.3",
		"eslint": "^8.31.0",
		"eslint-config-prettier": "^8.6.0",
		"eslint-plugin-prettier": "^4.2.1",
		"eslint-plugin-tsdoc": "^0.2.17",
		"prettier-plugin-jsdoc": "^0.4.2",
		"size-limit": "^8.1.0",
		"typescript": "^4.9.4",
		"vite": "^4.0.4",
		"vite-plugin-sdk": "^0.1.0",
		"vitest": "^0.26.3"
	},
	"engines": {
		"node": ">=14.15.0"
	},
	"publishConfig": {
		"access": "public"
	}
}
