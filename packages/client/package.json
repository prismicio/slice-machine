{
  "name": "@slicemachine/client",
  "version": "1.1.15",
  "main": "./build/index.js",
  "engines": {
    "node": ">=14"
  },
  "types": "./build/index.d.ts",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsc --project tsconfig.json",
    "watch": "tsc --project tsconfig.json --watch",
    "lint": "eslint --cache --cache-location .caches/eslint --cache-strategy content --ext .ts .",
    "lint:precommit": "eslint",
    "audit": "better-npm-audit audit -l high -p",
    "prepublishOnly": "npm run test && npm run build",
    "audit-fix": "npm audit fix",
    "test": "jest",
    "depcheck": "depcheck --config=.depcheckrc"
  },
  "dependencies": {
    "@prismicio/types-internal": "^1.5.2",
    "@slicemachine/core": "1.1.15",
    "axios": "^0.27.2",
    "form-data": "^3.0.0",
    "fp-ts": "^2.11.5",
    "io-ts": "^2.2.16",
    "mime": "^2.5.2"
  },
  "devDependencies": {
    "@babel/core": "^7.18.5",
    "@babel/runtime": "^7.18.3",
    "@types/jest": "^27.0.2",
    "@types/mime": "^2.0.3",
    "@types/node": "^16.10.5",
    "@typescript-eslint/eslint-plugin": "^5.10.0",
    "@typescript-eslint/parser": "^5.10.0",
    "better-npm-audit": "^3.4.0",
    "depcheck": "^1.4.3",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.4.0",
    "eslint-plugin-import": "^2.24.0",
    "eslint-plugin-jest": "^24.4.0",
    "jest": "^29.3.1",
    "nock": "^13.1.3",
    "ts-jest": "^29.0.3",
    "typescript": "^4.4.3"
  }
}
