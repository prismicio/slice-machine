{
	"name": "@prismicio/manager",
	"version": "0.0.1",
	"description": "Manage all aspects of a Prismic project.",
	"repository": {
		"type": "git",
		"url": "ssh://git@github.com/prismicio/devtools.git",
		"directory": "packages/manager"
	},
	"license": "Apache-2.0",
	"author": "Prismic <contact@prismic.io> (https://prismic.io)",
	"sideEffects": false,
	"type": "module",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"require": "./dist/index.cjs",
			"import": "./dist/index.js"
		},
		"./client": {
			"types": "./dist/client/index.d.js",
			"require": "./dist/client.cjs",
			"import": "./dist/client.js"
		},
		"./test": {
			"types": "./dist/test/index.d.js",
			"require": "./dist/test.cjs",
			"import": "./dist/test.js"
		},
		"./package.json": "./package.json"
	},
	"main": "dist/index.cjs",
	"module": "dist/index.js",
	"types": "dist/index.d.ts",
	"typesVersions": {
		"*": {
			"*": [
				"dist/index.d.ts"
			],
			"client": [
				"dist/client/index.d.ts"
			]
		}
	},
	"files": [
		"dist",
		"src"
	],
	"scripts": {
		"build": "vite build",
		"dev": "vite build --watch --mode development",
		"types": "tsc --noEmit",
		"format": "prettier --write .",
		"lint": "eslint --max-warnings 0 --ext .js,.ts .",
		"prepack": "$npm_execpath run build",
		"depcheck": "depcheck --config=.depcheckrc",
		"audit": "yarn npm audit --environment production --severity high"
	},
	"dependencies": {
		"@antfu/ni": "0.20.0",
		"@prismicio/client": "7.17.0",
		"@prismicio/custom-types-client": "2.1.0",
		"@prismicio/plugin-kit": "workspace:*",
		"@prismicio/types-internal": "3.11.2",
		"@segment/analytics-node": "2.1.2",
		"cookie": "1.0.1",
		"cors": "2.8.5",
		"execa": "7.1.1",
		"fp-ts": "2.13.1",
		"get-port": "6.1.2",
		"h3": "1.6.0",
		"io-ts": "2.2.20",
		"io-ts-reporters": "2.0.1",
		"io-ts-types": "0.5.19",
		"node-fetch": "3.3.1",
		"prettier": "3.0.3",
		"r19": "0.1.8",
		"rc9": "2.0.1",
		"semver": "7.3.8"
	},
	"devDependencies": {
		"@amplitude/experiment-node-server": "1.8.1",
		"@types/cors": "2.8.19",
		"@types/semver": "7.3.13",
		"@typescript-eslint/eslint-plugin": "5.55.0",
		"@typescript-eslint/parser": "5.55.0",
		"depcheck": "1.4.3",
		"eslint": "8.37.0",
		"eslint-config-prettier": "9.0.0",
		"eslint-plugin-prettier": "5.0.1",
		"eslint-plugin-tsdoc": "0.2.17",
		"msw": "1.1.0",
		"prettier-plugin-jsdoc": "1.1.1",
		"typescript": "4.9.5",
		"vite": "5.4.20",
		"vite-plugin-sdk": "0.1.1"
	},
	"peerDependencies": {
		"msw": ">=1.1.0"
	},
	"peerDependenciesMeta": {
		"msw": {
			"optional": true
		}
	},
	"engines": {
		"node": ">=20"
	},
	"publishConfig": {
		"access": "public"
	}
}
