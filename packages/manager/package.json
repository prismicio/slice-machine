{
	"name": "@slicemachine/manager",
	"version": "0.24.14",
	"description": "Manage all aspects of a Slice Machine project.",
	"repository": {
		"type": "git",
		"url": "ssh://git@github.com/prismicio/slice-machine.git",
		"directory": "packages/manager"
	},
	"license": "Apache-2.0",
	"author": "Prismic <contact@prismic.io> (https://prismic.io)",
	"sideEffects": false,
	"type": "module",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"require": "./dist/index.cjs",
			"import": "./dist/index.js"
		},
		"./client": {
			"types": "./dist/client/index.d.js",
			"require": "./dist/client.cjs",
			"import": "./dist/client.js"
		},
		"./test": {
			"types": "./dist/test/index.d.js",
			"require": "./dist/test.cjs",
			"import": "./dist/test.js"
		},
		"./package.json": "./package.json"
	},
	"main": "dist/index.cjs",
	"module": "dist/index.js",
	"types": "dist/index.d.ts",
	"typesVersions": {
		"*": {
			"*": [
				"dist/index.d.ts"
			],
			"client": [
				"dist/client/index.d.ts"
			],
			"test": [
				"dist/test/index.d.ts"
			]
		}
	},
	"files": [
		"dist",
		"src"
	],
	"scripts": {
		"build": "vite build",
		"dev": "vite build --watch --mode development",
		"format": "prettier --write .",
		"lint": "eslint --max-warnings 0 --ext .js,.ts .",
		"prepack": "$npm_execpath run build",
		"size": "size-limit",
		"test": "yarn lint && yarn types && yarn unit && yarn build && yarn size",
		"types": "tsc --noEmit",
		"unit": "vitest run --coverage",
		"unit:watch": "vitest watch",
		"depcheck": "depcheck --config=.depcheckrc",
		"audit": "yarn npm audit --environment production --severity high"
	},
	"dependencies": {
		"@antfu/ni": "^0.20.0",
		"@prismicio/client": "7.17.0",
		"@prismicio/custom-types-client": "2.1.0",
		"@prismicio/mocks": "2.13.0",
		"@prismicio/types-internal": "3.10.0",
		"@segment/analytics-node": "^2.1.2",
		"@slicemachine/plugin-kit": "workspace:*",
		"cookie": "^1.0.1",
		"cors": "^2.8.5",
		"execa": "^7.1.1",
		"file-type": "^18.2.1",
		"fp-ts": "^2.13.1",
		"get-port": "^6.1.2",
		"h3": "^1.6.0",
		"io-ts": "^2.2.20",
		"io-ts-reporters": "^2.0.1",
		"io-ts-types": "^0.5.19",
		"monocle-ts": "^2.3.13",
		"newtype-ts": "^0.3.5",
		"node-fetch": "^3.3.1",
		"p-limit": "^4.0.0",
		"prettier": "^3.0.3",
		"r19": "0.1.8",
		"rc9": "^2.0.1",
		"readable-web-to-node-stream": "^3.0.2",
		"semver": "^7.3.8"
	},
	"devDependencies": {
		"@amplitude/experiment-node-server": "1.8.1",
		"@prismicio/mock": "0.7.1",
		"@size-limit/preset-small-lib": "8.2.4",
		"@types/express": "4.17.17",
		"@types/semver": "7.3.13",
		"@typescript-eslint/eslint-plugin": "5.55.0",
		"@typescript-eslint/parser": "5.55.0",
		"@vitest/coverage-v8": "0.32.0",
		"depcheck": "1.4.3",
		"eslint": "8.37.0",
		"eslint-config-prettier": "9.0.0",
		"eslint-plugin-prettier": "5.0.1",
		"eslint-plugin-tsdoc": "0.2.17",
		"express": "4.20.0",
		"hook-std": "3.0.0",
		"memfs": "3.4.13",
		"msw": "1.1.0",
		"parse-multipart-data": "1.5.0",
		"prettier-plugin-jsdoc": "1.1.1",
		"size-limit": "8.2.4",
		"typescript": "4.9.5",
		"vite": "4.3.9",
		"vite-plugin-sdk": "0.1.1",
		"vitest": "0.32.0"
	},
	"peerDependencies": {
		"msw": "^1.1.0"
	},
	"peerDependenciesMeta": {
		"msw": {
			"optional": true
		}
	},
	"engines": {
		"node": ">=14.15.0"
	},
	"publishConfig": {
		"access": "public"
	}
}
