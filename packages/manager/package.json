{
	"name": "@slicemachine/manager",
	"version": "0.1.0",
	"private": true,
	"description": "Manage all aspects of a Slice Machine project.",
	"repository": {
		"type": "git",
		"url": "ssh://git@github.com/prismicio/slice-machine.git"
	},
	"license": "Apache-2.0",
	"author": "Prismic <contact@prismic.io> (https://prismic.io)",
	"sideEffects": false,
	"type": "module",
	"exports": {
		".": {
			"require": "./dist/index.cjs",
			"import": "./dist/index.js"
		},
		"./client": {
			"require": "./dist/client.cjs",
			"import": "./dist/client.js"
		},
		"./package.json": "./package.json"
	},
	"main": "dist/index.cjs",
	"module": "dist/index.js",
	"types": "dist/index.d.ts",
	"typesVersions": {
		"*": {
			"*": [
				"dist/index.d.ts"
			],
			"client": [
				"dist/client.d.ts"
			]
		}
	},
	"files": [
		"dist",
		"src"
	],
	"scripts": {
		"build": "vite build",
		"dev": "vite build --watch",
		"format": "prettier --write .",
		"lint": "eslint --ext .js,.ts .",
		"prepare": "npm run build",
		"size": "size-limit",
		"test": "npm run lint && npm run unit && npm run build && npm run size",
		"unit": "vitest run --coverage",
		"unit:watch": "vitest watch"
	},
	"dependencies": {
		"@prismicio/custom-types-client": "^1.0.1",
		"@prismicio/types": "^0.2.4",
		"@prismicio/types-internal": "^1.5.3",
		"@slicemachine/plugin-kit": "^0.1.7",
		"@wooorm/starry-night": "^1.4.2",
		"analytics-node": "^6.2.0",
		"busboy": "^1.6.0",
		"cookie": "^0.5.0",
		"cors": "^2.8.5",
		"file-type": "^18.0.0",
		"fp-ts": "^2.13.1",
		"get-port": "^6.1.2",
		"h3": "^1.0.2",
		"hast-util-to-string": "^2.0.0",
		"io-ts": "^2.2.20",
		"io-ts-reporters": "^2.0.1",
		"jiti": "^1.16.0",
		"node-fetch": "^3.3.0",
		"p-limit": "^4.0.0",
		"puppeteer": "^19.4.0",
		"r19": "^0.1.1",
		"rc9": "^2.0.0",
		"rehype-raw": "^6.1.1",
		"rehype-stringify": "^9.0.3",
		"remark-gfm": "^3.0.1",
		"remark-parse": "^10.0.1",
		"remark-rehype": "^10.1.0",
		"unified": "^10.1.2"
	},
	"devDependencies": {
		"@size-limit/preset-small-lib": "^8.1.0",
		"@trivago/prettier-plugin-sort-imports": "^4.0.0",
		"@types/analytics-node": "^3.1.9",
		"@types/busboy": "^1.5.0",
		"@types/cookie": "^0.5.1",
		"@types/prettier": "^2.7.1",
		"@typescript-eslint/eslint-plugin": "~5.46.1",
		"@typescript-eslint/parser": "^5.46.1",
		"@vitest/coverage-c8": "^0.25.8",
		"conditional-type-checks": "^1.0.6",
		"eslint": "^8.29.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-prettier": "^4.2.1",
		"eslint-plugin-tsdoc": "^0.2.17",
		"prettier": "^2.8.1",
		"prettier-plugin-jsdoc": "^0.4.2",
		"size-limit": "^8.1.0",
		"typescript": "^4.9.4",
		"vite": "^4.0.1",
		"vite-plugin-sdk": "^0.1.0",
		"vitest": "^0.25.8"
	},
	"engines": {
		"node": ">=14.15.0"
	},
	"publishConfig": {
		"access": "public"
	}
}
