{
	"name": "@slicemachine/plugin-kit",
	"version": "0.1.8-dev-plugins-validation.0",
	"description": "A set of helpers to develop and run Slice Machine plugins",
	"keywords": [
		"typescript",
		"helpers",
		"utils",
		"toolbox",
		"prismic"
	],
	"repository": {
		"type": "git",
		"url": "ssh://git@github.com/prismicio/slicemachine-plugin-kit.git",
		"directory": "packages/kit"
	},
	"license": "Apache-2.0",
	"author": "Prismic <contact@prismic.io> (https://prismic.io)",
	"type": "module",
	"exports": {
		".": {
			"require": "./dist/index.cjs",
			"import": "./dist/index.js"
		},
		"./package.json": "./package.json"
	},
	"main": "dist/index.cjs",
	"module": "dist/index.js",
	"types": "dist/index.d.ts",
	"files": [
		"dist",
		"src"
	],
	"scripts": {
		"build": "vite build",
		"dev": "vite build --watch",
		"format": "prettier --write .",
		"lint": "eslint --ext .js,.ts .",
		"//prepare": "npm run build",
		"release": "npm run test && standard-version && git push --follow-tags && npm run build && npm publish",
		"release:alpha": "npm run test && standard-version --release-as major --prerelease alpha && git push --follow-tags && npm run build && npm publish --tag alpha",
		"release:alpha:dry": "standard-version --release-as major --prerelease alpha --dry-run",
		"release:dry": "standard-version --dry-run",
		"size": "size-limit",
		"test": "npm run lint && npm run types && npm run unit && npm run build && npm run size",
		"types": "tsc --noEmit",
		"unit": "vitest run --coverage",
		"unit:watch": "vitest watch",
		"depcheck": "depcheck --config=.depcheckrc",
		"audit": "better-npm-audit audit -l high -p"
	},
	"dependencies": {
		"@prismicio/types": "^0.2.7",
		"common-tags": "^1.8.2",
		"defu": "^6.1.2",
		"fp-ts": "^2.13.1",
		"io-ts": "^2.2.20",
		"io-ts-reporters": "^2.0.1",
		"prettier": "^2.8.4"
	},
	"devDependencies": {
		"@prismicio/mock": "^0.2.0",
		"@size-limit/preset-small-lib": "^8.2.4",
		"@types/common-tags": "^1.8.1",
		"@types/prettier": "^2.7.2",
		"@typescript-eslint/eslint-plugin": "^5.55.0",
		"@typescript-eslint/parser": "^5.55.0",
		"@vitest/coverage-c8": "^0.27.3",
		"better-npm-audit": "^3.7.3",
		"eslint": "^8.36.0",
		"eslint-config-prettier": "^8.7.0",
		"eslint-plugin-prettier": "^4.2.1",
		"eslint-plugin-tsdoc": "^0.2.17",
		"prettier": "^2.8.4",
		"prettier-plugin-jsdoc": "^0.4.2",
		"size-limit": "^8.2.4",
		"standard-version": "^9.5.0",
		"ts-expect": "^1.3.0",
		"typescript": "^4.9.5",
		"vite": "^4.1.4",
		"vite-plugin-sdk": "^0.1.1",
		"vitest": "^0.27.3"
	},
	"engines": {
		"node": ">=14.15.0"
	},
	"publishConfig": {
		"access": "public"
	}
}
