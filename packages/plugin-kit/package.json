{
	"name": "@prismicio/plugin-kit",
	"version": "0.0.1",
	"description": "A set of helpers to develop and run Prismic plugins",
	"keywords": [
		"typescript",
		"helpers",
		"utils",
		"toolbox",
		"prismic"
	],
	"repository": {
		"type": "git",
		"url": "ssh://git@github.com/prismicio/devtools.git",
		"directory": "packages/plugin-kit"
	},
	"license": "Apache-2.0",
	"author": "Prismic <contact@prismic.io> (https://prismic.io)",
	"type": "module",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"require": "./dist/index.cjs",
			"import": "./dist/index.js"
		},
		"./fs": {
			"types": "./dist/fs/index.d.ts",
			"require": "./dist/fs/index.cjs",
			"import": "./dist/fs/index.js"
		},
		"./package.json": "./package.json"
	},
	"main": "dist/index.cjs",
	"module": "dist/index.js",
	"types": "dist/index.d.ts",
	"typesVersions": {
		"*": {
			"*": [
				"dist/index.d.ts"
			],
			"fs": [
				"dist/fs/index.d.ts"
			]
		}
	},
	"files": [
		"dist",
		"src"
	],
	"engines": {
		"node": ">=20.19.0"
	},
	"publishConfig": {
		"access": "public"
	},
	"scripts": {
		"build": "vite build",
		"dev": "vite build --watch",
		"types": "tsc --noEmit",
		"format": "prettier --write .",
		"lint": "eslint --max-warnings 0 --ext .js,.ts .",
		"prepack": "$npm_execpath run build",
		"depcheck": "depcheck --config=.depcheckrc",
		"audit": "yarn npm audit --environment production --severity high"
	},
	"dependencies": {
		"@prismicio/client": "7.17.0",
		"common-tags": "1.8.2",
		"defu": "6.1.2",
		"fp-ts": "2.13.1",
		"fs-extra": "11.1.1",
		"io-ts": "2.2.20",
		"io-ts-reporters": "2.0.1",
		"p-limit": "4.0.0",
		"prettier": "3.7.1",
		"prismic-ts-codegen": "0.1.27"
	},
	"devDependencies": {
		"@prismicio/types-internal": "3.11.2",
		"@types/common-tags": "1.8.1",
		"@types/fs-extra": "11.0.1",
		"@typescript-eslint/eslint-plugin": "5.55.0",
		"@typescript-eslint/parser": "5.55.0",
		"depcheck": "1.4.3",
		"eslint": "8.37.0",
		"eslint-config-prettier": "9.0.0",
		"eslint-plugin-prettier": "5.0.1",
		"eslint-plugin-tsdoc": "0.2.17",
		"io-ts-types": "0.5.19",
		"prettier-plugin-jsdoc": "1.7.0",
		"typescript": "4.9.5",
		"vite": "5.4.20",
		"vite-plugin-sdk": "0.1.1"
	}
}
