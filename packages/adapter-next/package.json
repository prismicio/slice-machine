{
	"name": "@prismicio/adapter-next",
	"version": "0.0.1",
	"description": "Prismic adapter for Next.js.",
	"keywords": [
		"typescript",
		"prismic"
	],
	"repository": {
		"type": "git",
		"url": "ssh://git@github.com/prismicio/devtools.git",
		"directory": "packages/adapter-next"
	},
	"license": "Apache-2.0",
	"author": "Prismic <contact@prismic.io> (https://prismic.io)",
	"type": "module",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"require": "./dist/index.cjs",
			"import": "./dist/index.js"
		},
		"./package.json": "./package.json"
	},
	"main": "dist/index.cjs",
	"module": "dist/index.js",
	"types": "dist/index.d.ts",
	"typesVersions": {
		"*": {
			"*": [
				"dist/index.d.ts"
			]
		}
	},
	"files": [
		"dist",
		"src"
	],
	"scripts": {
		"build": "vite build",
		"dev": "vite build --watch",
		"format": "prettier --write .",
		"lint": "eslint --max-warnings 0 --ext .js,.ts .",
		"prepack": "$npm_execpath run build",
		"test": "yarn lint && yarn types && yarn build",
		"types": "tsc --noEmit",
		"depcheck": "depcheck --config=.depcheckrc",
		"audit": "yarn npm audit --environment production --severity high"
	},
	"dependencies": {
		"@prismicio/plugin-kit": "workspace:*",
		"common-tags": "1.8.2",
		"monocle-ts": "2.3.13",
		"newtype-ts": "0.3.5",
		"pascal-case": "3.1.2"
	},
	"devDependencies": {
		"@types/common-tags": "1.8.1",
		"@typescript-eslint/eslint-plugin": "5.55.0",
		"@typescript-eslint/parser": "5.55.0",
		"@vitejs/plugin-react": "3.1.0",
		"depcheck": "1.4.3",
		"eslint": "8.37.0",
		"eslint-config-prettier": "9.0.0",
		"eslint-plugin-prettier": "5.0.1",
		"eslint-plugin-tsdoc": "0.2.17",
		"next": "16.0.0-beta.0",
		"prettier": "3.0.3",
		"prettier-plugin-jsdoc": "1.1.1",
		"react": "19.2.0",
		"rollup-plugin-preserve-directives": "0.4.0",
		"typescript": "4.9.5",
		"vite": "5.4.20",
		"vite-plugin-sdk": "0.1.1"
	},
	"peerDependencies": {
		"next": "^11 || ^12 || ^13 || ^14 || ^15 || ^16.0.0-beta.0",
		"react": "^17 || ^18 || ^19.0.0-rc.0"
	},
	"engines": {
		"node": ">=14.15.0"
	},
	"publishConfig": {
		"access": "public"
	}
}
