{
	"name": "@prismicio/adapter-next",
	"version": "0.0.1",
	"description": "Prismic adapter for Next.js.",
	"keywords": [
		"typescript",
		"prismic"
	],
	"repository": {
		"type": "git",
		"url": "ssh://git@github.com/prismicio/devtools.git",
		"directory": "packages/adapter-next"
	},
	"license": "Apache-2.0",
	"author": "Prismic <contact@prismic.io> (https://prismic.io)",
	"type": "module",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.js"
		},
		"./package.json": "./package.json"
	},
	"main": "dist/index.js",
	"module": "dist/index.js",
	"types": "dist/index.d.ts",
	"typesVersions": {
		"*": {
			"*": [
				"dist/index.d.ts"
			]
		}
	},
	"files": [
		"dist",
		"src"
	],
	"engines": {
		"node": ">=20.19.0"
	},
	"publishConfig": {
		"access": "public"
	},
	"scripts": {
		"build": "vite build",
		"dev": "vite build --watch",
		"types": "tsc --noEmit",
		"format": "prettier --write .",
		"lint": "eslint --max-warnings 0 .",
		"prepack": "$npm_execpath run build",
		"depcheck": "depcheck --config=.depcheckrc",
		"audit": "yarn npm audit --environment production --severity high"
	},
	"dependencies": {
		"@prismicio/plugin-kit": "workspace:*",
		"common-tags": "1.8.2",
		"pascal-case": "3.1.2"
	},
	"devDependencies": {
		"@eslint/js": "9.28.0",
		"@types/common-tags": "1.8.4",
		"@vitejs/plugin-react": "5.1.1",
		"depcheck": "1.4.7",
		"eslint": "9.39.1",
		"eslint-config-prettier": "10.1.8",
		"eslint-plugin-tsdoc": "0.5.0",
		"next": "16.0.5",
		"prettier": "3.7.1",
		"prettier-plugin-jsdoc": "1.7.0",
		"react": "19.2.0",
		"rollup-plugin-preserve-directives": "0.4.0",
		"typescript": "5.9.3",
		"typescript-eslint": "8.33.0",
		"vite": "7.2.4",
		"vite-plugin-sdk": "0.1.5"
	},
	"peerDependencies": {
		"next": "^11 || ^12 || ^13 || ^14 || ^15 || ^16.0.0-beta.0",
		"react": "^17 || ^18 || ^19.0.0-rc.0"
	}
}
